{
    "extends": "@vue/tsconfig/tsconfig.json",
    "compilerOptions": {
        "lib": ["ESNext", "DOM"],
        "allowJs": true,
        "sourceMap": true,
        "baseUrl": ".",
        "paths": {
            "@/*": ["./src/*"]
        },
        "types": ["@dcloudio/types"]
    },
    "include": ["src/**/*.ts", "src/**/*.d.ts", "src/**/*.tsx", "src/**/*.vue"],
    "exclude": ["node_modules", "dist/"]
}
